{
	"name": "kompiler",
	"version": "5.56",
	"bin": {
		"kompiler": "./index.js"
	},
	"dependencies": {
		"@rollup/plugin-babel": "^5.3.1",
		"@rollup/plugin-commonjs": "^21.0.3",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-multi-entry": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@rollup/plugin-replace": "^3.0.0",
		"babel-preset-solid": "^1.3.13",
		"better-sqlite3": "^7.5.0",
		"esm": "^3.2.25",
		"express": "^4.17.3",
		"glob": "7.2.0",
		"imagemin": "^7.0.1",
		"imagemin-gifsicle": "7.0.0",
		"imagemin-jpegtran": "7.0.0",
		"imagemin-optipng": "8.0.0",
		"mime-types": "^2.1.35",
		"postcss-import": "^14.1.0",
		"prompt-sync": "^4.2.0",
		"request": "2.88.2",
		"rollup": "^2.70.1",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-terser": "^7.0.2",
		"solid-js": "^1.3.13",
		"ws": "^7.5.6"
	},
	"prettier": {
		"printWidth": 100,
		"useTabs": true,
		"semi": false,
		"singleQuote": true,
		"jsxSingleQuote": false,
		"trailingComma": "all",
		"bracketSpacing": true,
		"jsxBracketSameLine": false,
		"arrowParens": "avoid",
		"quoteProps": "preserve",
		"proseWrap": "preserve"
	},
	"eslintConfig": {
		"env": {
			"browser": true,
			"node": true,
			"es6": true
		},
		"parser": "babel-eslint",
		"extends": "eslint:recommended",
		"parserOptions": {
			"ecmaFeatures": {
				"experimentalObjectRestSpread": true,
				"jsx": true,
				"impliedStrict": true,
				"es6": true
			},
			"es6": true,
			"ecmaVersion": 6,
			"sourceType": "module",
			"allowImportExportEverywhere": false,
			"codeFrame": false
		},
		"plugins": ["react"],
		"rules": {
			"comma-dangle": "off",
			"indent": "off",
			"linebreak-style": ["error", "unix"],
			"no-cond-assign": "off",
			"no-console": "off",
			"no-empty": "off",
			"no-extra-boolean-cast": "off",
			"no-extra-semi": "off",
			"no-fallthrough": "off",
			"no-func-assign": "off",
			"no-inner-declarations": "off",
			"no-mixed-spaces-and-tabs": "off",
			"no-redeclare": "off",
			"no-tabs": "off",
			"no-undef": "off",
			"no-global-assign": "off",
			"no-unreachable": "off",
			"no-unused-vars": "off",
			"strict": "off"
		}
	}
}
